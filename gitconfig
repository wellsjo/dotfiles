[alias]
	co = checkout
	br = branch
	pushf = push --force-with-lease
[icdiff]
	options = --highlight --line-numbers
[pager]
	branch = false
[url "git@github.com:"]
	insteadOf = https://github.com/
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[init]
	defaultBranch = master
